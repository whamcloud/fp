{
  "name": "@mfl/fp",
  "version": "5.1.0",
  "description": "A Functional Programming Lib.",
  "main": "dist/source/fp.js",
  "scripts": {
    "eslint": "eslint ./",
    "test:dev": "npm run postversion && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "postversion": "./bin/build.sh",
    "cover:dev": "npm run cover -- --reporter=html",
    "cover": "NODE_ENV=test ./bin/build.sh cover && nyc --reporter=text npm t"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": [
    "eslint",
    "test:dev"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:intel-hpdd/fp.git"
  },
  "author": "MFL Team",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-flow-strip-types": "6.8.0",
    "babel-plugin-transform-strict-mode": "6.11.3",
    "babel-preset-es2015": "6.13.2",
    "eslint": "3.3.1",
    "eslint-plugin-flowtype": "2.7.0",
    "immutable": "3.8.1",
    "@mfl/flow-jasmine": "1.5.0",
    "@mfl/jasmine-n-matchers": "2.1.1",
    "jasmine": "2.4.1",
    "nyc": "8.1.0",
    "pre-commit": "1.1.3"
  }
}
